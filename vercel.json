{
  "version": 2,
  "builds": [
    {
      "src": "FashionHub/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "FashionHub/backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "FashionHub/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "FashionHub/frontend/build/index.html"
    }
  ]
}